<% @artworks.each do |artwork| %>
  <div class="courses-box courses-box-slick">
    <div class="card course-card card-course-slick" style="background-image: url('<%= cl_image_path artwork.photo? ? artwork.photo.path : 'logo.png', crop: :fill %>');">
      <div class="card-category card-category-slick"><%# artwork.message %></div>
      <%= link_to "", studio_path, class: "card-link"  %>
      <div class="card-course-name-slick"><h2><%= artwork.message %></h2></div>
    </div>
  </div>
<% end %>
