<%- if devise_mapping.omniauthable? %>
  <%- resource_class.omniauth_providers.each do |provider| %>
    <%= link_to "Sign in with #{OmniAuth::Utils.camelize(provider)}", omniauth_authorize_path(resource_name, provider), { :class => "fa fa-facebook btn btn-block btn-social btn-facebook" } %><br />
  <% end -%>
<% end -%>
